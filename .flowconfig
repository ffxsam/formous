[options]
# Absolute path support:
# e.g. "/collections/todos"
module.name_mapper='^\/\(.*\)$' -> '<PROJECT_ROOT>/\1'
esproposal.class_instance_fields=enable

[ignore]
# Ignore problematic packages in node_modules
.*/node_modules/cjson/.*
.*/node_modules/fbjs/.*
.*/node_modules/react-tap-event-plugin/.*
